<ul id="search_autocomplete" class="ui-autocomplete ui-menu ui-widget ui-widget-content ui-corner-all b-search-dropdown_group-autocomplete">
    <% if @items: %>
    <% for item in @items: %>
        <li class="ui-menu-item">
            <a  class="ui-corner-all item"
                data-class="<%= item.class %>"
                data-href="<%= item.href %>">
                    <%= item.label %>
            </a>
        </li>
    <% end %>
    <% end %>
</ul>
